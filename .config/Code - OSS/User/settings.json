{
    "editor.fontFamily": "'Iosevka', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "workbench.colorTheme": "Tomorrow Night",
    "keyboard.dispatch": "keyCode",
    "editor.fontSize": 16,
    "vetur.format.scriptInitialIndent": true,
    "vetur.format.styleInitialIndent": true,
    "editor.highlightActiveIndentGuide": false,
    "editor.tabSize": 2,
    "java.semanticHighlighting.enabled": true,
    // "java.project.referencedLibraries": [
    //     "/home/mustaqim/Downloads/perimeter/*.jar"
    // ],
    "java.home": "/usr/lib/jvm/jbrsdk",
    "telemetry.enableTelemetry": false,
    "editor.renderIndentGuides": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.fontLigatures": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "vscode-neovim.neovimPath": "/bin/nvim",
    "workbench.iconTheme": "vscode-great-icons",
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "foreground": "#1d1f21",
            "background": "#f0c674",
            "icon": "checkbox",
            "type": "tag"
        }
    }
}